{
  "name": "bff-login",
  "version": "1.0.17",
  "description": "",
  "author": "",
  "scripts": {
    "copy-assets": "mv public  ./dist/src/",
    "prebuild": "rm -rf dist && npm run build:client",
    "build": "nest build && npm run copy-assets",
    "format": "prettier --write 'src/**/*.ts'",
    "prestart": "npm run build",
    "start": "node dist/src/main.js",
    "build:client": "cd ../client && npm run build",
    "prestart:test": "npm run build",
    "start:test": "node -r dotenv/config ./dist/src/main.js dotenv_config_path=./.env.test",
    "prestart:dev": "rm -rf dist && npm run build:client && npm run copy-assets",
    "start:dev": "nest start --watch -e 'node -r dotenv/config' -- dotenv_config_path=./.env.dev",
    "start:debug": "npm run start:dev -- --debug",
    "start:server:dev": "nest start --watch -e 'node -r dotenv/config' -- dotenv_config_path=./.env.dev",
    "start:server:debug": "npm run start:server:dev -- --debug",
    "lint": "./node_modules/.bin/eslint 'src/**/*.ts' ",
    "lint:fix": "npm run lint -- --fix",
    "test": "./node_modules/.bin/jest --detectOpenHandles --config ./jest.config.ts",
    "test:ci": "npm run test -- --ci",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "express-session": "^1.17.1",
    "hbs": "^4.1.2",
    "module-alias": "^2.2.2",
    "nestjs-redis": "^1.3.3",
    "nestjs-session": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@golevelup/nestjs-testing": "^0.1.2",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jest-junit": "^12.2.0",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
